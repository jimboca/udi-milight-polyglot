<editors>

    <!-- Bridge connected  -->
    <editor id="bool">
        <range uom="2" subset="0,1" />
    </editor>

    <editor id="MONOFF">
        <range uom="78" subset="0,100" />
    </editor>
    
    <!-- Color 0 to 255 Value -->
    <editor id="MCOLOR">
        <range uom="100" min="0" max="255" step="1" prec="0" />
    </editor>
    
    <!-- Brightness Editor -->
    <editor id="MCLBRI">
        <range uom="51" min="1" max="100" prec="0" step="1" />
    </editor>

    <editor id="MCLSAT">
        <range uom="51" min="1" max="100" prec="0" step="1" />
    </editor>
    
    <!-- Effect Selector -->
    <editor id="MEFFECT">
        <range uom="25" subset="1-9" nls="COLORLOOP" />
    </editor>
    
    <!-- Kelvin Editor -->
    <editor id="MCTEMP">
        <range uom="25" subset="1-5" nls="TEMP_SEL"/>
    </editor>
    
    <!-- Color Picker Editor -->
    <editor id="MCOLORPICK">
       <range uom="25" subset="1-8" nls="COLOR_SEL" />
    </editor>
        
</editors>
